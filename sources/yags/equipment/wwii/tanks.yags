    <item name="M-3 Halftrack APC">
        <category>Vehicle</category>
        <genre>WWII</genre>
        <techLevel>6</techLevel>
        <legality>2</legality>
        <availability>2</availability>
        <importance>3</importance>

        <short>Light infantry transport.</short>
        <cost>2600</cost>
        <weight>10200000</weight>

        <construction>
            <manufacturer>US</manufacturer>
            <year>1942</year>
        </construction>

        <description>
            <para>
                A half-tracked armoured personel carrier with an open
                top.
            </para>
        </description>

        <performance realm="ground">
            <speed>64</speed>
            <acceleration>3</acceleration>
            <range>450</range>
        </performance>

        <vehicle xmlns="http://yagsbook.sourceforge.net/xml/yags">

            <attributes size="12" soak="18" move="65" acceleration="10">
                <attribute name="strength" score="56"/>
                <attribute name="health" score="4"/>
                <attribute name="agility" score="3"/>
            </attributes>

            <combat>
                <armour score="15"/>

                <weapon name="Heavy machine gun">
                    <attack score="15"/>
                    <damage score="45"/>
                    <increment>30</increment>
                    <rof>10</rof><capacity>700</capacity>
                    <recoil>-10</recoil>
                    <range>
                        <short>1000</short>
                        <medium>2000</medium>
                        <long>4000</long>
                    </range>
                    <properties>
                        <firearm/><heavy/><automatic/>
                    </properties>
                </weapon>

                <weapon name="Light Machine Gun">
                    <attack score="15"/>
                    <damage score="35"/>
                    <increment>25</increment>
                    <rof>10</rof><capacity>7000</capacity>
                    <recoil>-5</recoil>
                    <range>
                        <short>750</short>
                        <medium>2000</medium>
                        <long>4000</long>
                    </range>
                    <properties>
                        <firearm/><heavy/><automatic/>
                    </properties>
                </weapon>
            </combat>
        </vehicle>
    </item>

