<equipment xmlns="http://yagsbook.sourceforge.net/xml">

    <item name="Hand axe">
        <category>Weapon</category>
        <importance>3</importance>

        <short>A small weapon designed to be used in one hand.</short>

        <description>
            <para>
                A small axe, not designed specifically for combat but
                can be used in close quarters fighting. It has a small
                head, with a haft about 30cm long.
            </para>
        </description>

        <weight>250</weight>
        <material>metal</material>

        <cost units="saxon">40</cost>

        <weapon xmlns="http://yagsbook.sourceforge.net/xml/yags">
            <category>Axes</category>
            <class>single</class>

            <combat>
                <attack>+2</attack>
                <defence>+1</defence>
                <damage>+5</damage>
            </combat>

            <strength>1</strength>
            <load>0.5</load>
            <reach>1</reach>

            <properties>
                <heavy/>
                <throwable/>
            </properties>
